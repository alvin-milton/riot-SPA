<built-with>

  <style>
    .bw {
      position: absolute;
      bottom: 10px;
      right: 10px;
    }
  </style>

  <div class="bw">
    <img src="{ this.src }" alt="{ this.componentTitle }">
  </div>

  <script>

    this.src = 'http://riotjs.com/img/logo/riot60x.png'
    this.componentTitle = 'riot logo'

  </script>

</built-with>
